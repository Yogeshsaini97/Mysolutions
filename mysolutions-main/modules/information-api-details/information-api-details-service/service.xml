<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.info.api.details">
	<namespace>FOO</namespace>
	<entity name="APIDetails" local-service="true" remote-service="true" uuid="true">

		<column name="ID" primary="true" type="long" />
		<column name="CreateDate" type="Date" />

		<column name="UserID" type="long" />
		<column name="Path" type="String" />
		<column name="Method" type="String" />
		<column name="RequestBody" type="String" />
		<column name="ResponseCode" type="int" />
		<column name="ResponseMessage" type="String" />

	
	</entity>
</service-builder>